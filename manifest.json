{
  "manifest_version": 3,
  "name": "Garmin Data Extractor",
  "version": "0.1.1",
  "author": "Gergely Imreh",

  "description": "Exports your data from the Garmin Connect website.",

  "action": {
    "default_icon": "icons/garmin-extractor-48.png"
  },

  "icons": {
    "48": "icons/garmin-extractor-48.png",
    "128": "icons/garmin-extractor-128.png",
  }

  "content_scripts": [
    {
      "matches": ["https://connect.garmin.com/*"],
      "js": ["heartrate.js"]
    }
  ],

  "permissions": [
  ]
}